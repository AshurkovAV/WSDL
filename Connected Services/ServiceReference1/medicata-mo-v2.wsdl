                  </xs:annotation>
                </xs:element>
                <xs:element name="Quantity" type="sch0:n1..10" />
                <xs:element name="PackageQuantity" type="sch0:n1..8" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="MedicationCost" type="sch0:n1..10">
            <xs:annotation>
              <xs:documentation>Стоимость отпущенного ЛП</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
